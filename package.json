{
  "name": "nextspace",
  "version": "1.0.0",
  "license": "AGPL-3.0-only",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10",
    "yarn": "please-use-npm"
  },
  "type": "module",
  "scripts": {
    "commit": "cz",
    "build": "next build",
    "dev": "next dev -p 8080",
    "docs": "typedoc --plugin typedoc-material-theme --themeColor '#cb9820' --out ./docs ./types.ts ./components/index.ts ./utils/index.ts",
    "lint": "next lint",
    "prepare": "husky",
    "openapi-types:generate:local": "openapi-typescript http://localhost:3000/v1/openapi.json --output types.ts",
    "openapi-types:generate": "sh -c 'if [ -z \"$1\" ]; then echo \"Error: URL to OpenAPI file required\" >&2; exit 1; fi; openapi-typescript \"$1\" --output types.ts' --",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "7.2.0",
    "@mui/lab": "7.0.0-beta.14",
    "@mui/material": "^7.1.2",
    "@mui/material-nextjs": "^7.0.2",
    "@mui/x-date-pickers": "^8.9.0",
    "@types/react-scroll": "^1.8.10",
    "dayjs": "^1.11.13",
    "dotenv": "17.2.3",
    "jose": "6.1.1",
    "linkify-react": "^4.3.2",
    "motion": "^12.12.1",
    "next": "^15.5.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-scroll": "^1.9.3",
    "remark-gfm": "^4.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tailwindcss/postcss": "^4.1.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9",
    "eslint-config-next": "15.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-mocks-http": "^1.17.2",
    "openapi-typescript": "^7.10.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.13",
    "typedoc-github-theme": "^0.3.1",
    "typedoc-material-theme": "^1.4.0",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
